[alias]
	st = status
	cm = commit -m
	ck = checkout
	cb = checkout -b
	ba = branch -a
	br = branch
	bd = branch -D
	pbd = push origin --delete
	mg = merge --no-ff -m
	clog = log --graph --pretty=oneline --abbrev-commit
	po = push origin
	pl = pull origin
	pm = pull origin master
	pts = push --tags
	rp = remote prune origin
	
# git push origin tagname
# git push origin --tags
# git remote prune origin  清除已经删除的远程分支的本地记录	